<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xml:tools="http://schemas.android.com/tools"
    android:id="@+id/container_help"
    android:layout_width="1920px"
    android:layout_height="1200px"
    android:background="@drawable/container_help">

  <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/container_go_back_bottom"
      android:layout_width="455px"
      android:layout_height="125px"
      android:background="?attr/selectableItemBackground"
      android:clickable="true"
      android:focusable="true"
      android:onClick="onBackButtonClicked1"
      tools:ignore="MissingConstraints">

    <ImageView
        android:id="@+id/image_go_back"
        android:layout_width="455px"
        android:layout_height="113px"
        android:background="@drawable/image_go_back"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

      <ImageView
          android:id="@+id/image_go_back1"
          android:layout_width="436px"
          android:layout_height="99px"
          android:background="@drawable/image_go_back1"
          android:scaleType="centerCrop"
          app:layout_constraintBottom_toBottomOf="@+id/image_go_back"
          app:layout_constraintTop_toTopOf="@+id/image_go_back"
          tools:layout_editor_absoluteX="4dp" />

      <TextView
          android:id="@+id/text_go_back_title"
          android:layout_width="198dp"
          android:layout_height="44dp"
          android:gravity="center"
          android:includeFontPadding="false"
          android:maxLines="1"
          android:singleLine="true"
          android:text="回上一頁"
          android:textColor="#FFDCF2F1"
          android:textSize="64px" />

      <ImageView
          android:id="@+id/container_back_icon"
          android:layout_width="96px"
          android:layout_height="77px"
          android:scaleType="centerInside"
          android:src="@drawable/container_back_icon"
          app:layout_constraintBottom_toBottomOf="@+id/image_go_back1"
          app:layout_constraintHorizontal_bias="0"
          app:layout_constraintEnd_toEndOf="@+id/image_go_back1"
          app:layout_constraintStart_toStartOf="@+id/image_go_back1"
          app:layout_constraintTop_toTopOf="@+id/image_go_back1" />

  </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/button_not_comfortable_container"
        android:layout_width="272dp"
        android:layout_height="382dp"
        android:clickable="true"
        android:focusable="true"
        android:onClick="onButtonClicked"
        tools:layout_editor_absoluteX="414dp"
        tools:layout_editor_absoluteY="51dp"
        tools:ignore="MissingConstraints">


        <ImageView
            android:id="@+id/image_not_comfortable"
            android:layout_width="694.91px"
            android:layout_height="976.84px"
            android:background="@drawable/image_not_comfortable"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/image_not_comfortable1"
            android:layout_width="694.91px"
            android:layout_height="976.84px"
            android:background="@drawable/image_not_comfortable1"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_not_comfortable_title"
            android:layout_width="241px"
            android:layout_height="680px"
            android:layout_marginBottom="8dp"
            android:gravity="center_horizontal|top"
            android:includeFontPadding="false"
            android:lineHeight="128px"
            android:text="我\n不\n舒\n服"
            android:textColor="#FF0F1035"
            android:textSize="128px"
            app:layout_constraintBottom_toBottomOf="@+id/image_not_comfortable1"
            app:layout_constraintEnd_toEndOf="@+id/image_not_comfortable1"
            app:layout_constraintHorizontal_bias="0.526"
            app:layout_constraintStart_toStartOf="@+id/image_not_comfortable1" />

        <ImageView
            android:id="@+id/image_not_comfortable_vector"
            android:layout_width="226.34px"
            android:layout_height="218.01px"
            android:scaleType="centerInside"
            android:src="@drawable/image_not_comfortable_vector"
            app:layout_constraintBottom_toBottomOf="@+id/image_not_comfortable1"
            app:layout_constraintEnd_toEndOf="@+id/image_not_comfortable1"
            app:layout_constraintHorizontal_bias="0.513"
            app:layout_constraintStart_toStartOf="@+id/image_not_comfortable1"
            app:layout_constraintTop_toTopOf="@+id/image_not_comfortable1"
            app:layout_constraintVertical_bias="0.041" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/button_course_recommould_container"
        android:layout_width="273dp"
        android:layout_height="378dp"
        android:clickable="true"
        android:focusable="true"
        android:onClick="onCourseRecomButtonClicked"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="69dp"
        tools:layout_editor_absoluteY="53dp">

        <ImageView
            android:id="@+id/image_course_recommend"
            android:layout_width="694.91px"
            android:layout_height="976.84px"
            android:background="@drawable/image_course_recommend"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/image_course_recommend1"
            android:layout_width="694.91px"
            android:layout_height="976.84px"
            android:background="@drawable/image_course_recommend1"
            android:scaleType="centerCrop"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_course_recommend_title"
            android:layout_width="241px"
            android:layout_height="680px"
            android:gravity="center_horizontal|top"
            android:includeFontPadding="false"
            android:lineHeight="128px"
            android:text="課\n程\n推\n薦"
            android:textColor="#FF0F1035"
            android:textSize="128px"
            app:layout_constraintBottom_toBottomOf="@+id/image_course_recommend1"
            app:layout_constraintEnd_toEndOf="@+id/image_course_recommend1"
            app:layout_constraintHorizontal_bias="0.473"
            app:layout_constraintStart_toStartOf="@+id/image_course_recommend1"
            app:layout_constraintTop_toTopOf="@+id/image_course_recommend1"
            app:layout_constraintVertical_bias="0.831" />

        <ImageView
            android:id="@+id/image_course_recommend_face_fun"
            android:layout_width="86dp"
            android:layout_height="87dp"
            android:scaleType="centerInside"
            android:src="@drawable/image_course_recommend_face_fun"
            app:layout_constraintBottom_toBottomOf="@+id/image_course_recommend1"
            app:layout_constraintEnd_toEndOf="@+id/image_course_recommend1"
            app:layout_constraintHorizontal_bias="0.488"
            app:layout_constraintStart_toStartOf="@+id/image_course_recommend1"
            app:layout_constraintTop_toTopOf="@+id/image_course_recommend1"
            app:layout_constraintVertical_bias="0.038" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
